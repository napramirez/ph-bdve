<?xml version="1.0" encoding="UTF-8" standalone="yes"?><ns2:ContextValueAssociation xmlns:ns2="http://docs.oasis-open.org/codelist/ns/ContextValueAssociation/1.0/" name="OPENPEPPOL-T19"><ValueLists><ValueList uri="../gc/OPENPEPPOL-ActionCodePEPPOL.gc" xml:id="ActionCodePEPPOL"/><ValueList uri="../gc/OPENPEPPOL-AttributeCode.gc" xml:id="AttributeCode"/><ValueList uri="../gc/OPENPEPPOL-CountryCodeList.gc" xml:id="CountryCodeList"/><ValueList uri="../gc/OPENPEPPOL-CurrencyCodeList.gc" xml:id="CurrencyCodeList"/><ValueList uri="../gc/OPENPEPPOL-Language.gc" xml:id="Language"/><ValueList uri="../gc/OPENPEPPOL-PackageLevelCode.gc" xml:id="PackageLevelCode"/><ValueList uri="../gc/OPENPEPPOL-PartyIdentifierScheme.gc" xml:id="PartyIdentifierScheme"/><ValueList uri="../gc/OPENPEPPOL-UNDG.gc" xml:id="UNDG"/><ValueList uri="../gc/OPENPEPPOL-UnitCode.gc" xml:id="UnitCode"/></ValueLists><Contexts><Context mark="fatal" values="CountryCodeList" address="cac:Country/cbc:IdentificationCode"><Message>[OP-T19-R001]-Country codes in an invoice MUST be coded using ISO code list 3166-1</Message></Context><Context mark="fatal" values="UnitCode" address="@unitCode"><Message>[OP-T19-R002]-Unit code MUST be coded according to the UN/ECE Recommendation 20</Message></Context><Context mark="fatal" values="UNDG" address="cbc:UNDGCode"><Message>[OP-T19-R003]-UNDG MUST be coded according to the UNCL 8273 code list</Message></Context><Context mark="fatal" values="PartyIdentifierScheme" address="cbc:EndpointID/@schemeID"><Message>[OP-T19-R005]-An Endpoint Identifier Scheme MUST be from the list of PEPPOL Party Identifiers described in the &quot;PEPPOL Policy for using Identifiers&quot;.</Message></Context><Context mark="fatal" values="PartyIdentifierScheme" address="cac:PartyIdentification/cbc:ID/@schemeID"><Message>[OP-T19-R006]-A Party Identifier Scheme MUST be from the list of PEPPOL Party Identifiers described in the &quot;PEPPOL Policy for using Identifiers&quot;.</Message></Context><Context mark="fatal" values="ActionCodePEPPOL" address="/ubl:Catalogue/cbc:ActionCode/"><Message>[OP-T19-R007]-An action code at header level MUST be from the PEPPOL action code list.</Message></Context><Context mark="fatal" values="PackageLevelCode" address="cbc:PackageLevelCode"><Message>[OP-T19-R008]-A package level code MUST be from the PEPPOL subset of GS1 7009 code list.</Message></Context><Context mark="fatal" values="AttributeCode" address="cbc:AttributeID"><Message>[OP-T19-R009]-An attribute identifier MUST be from the UN EDIFACT 6313 code list</Message></Context><Context mark="fatal" values="UnitCode" address="cbc:OrderableUnit"><Message>[OP-T19-R010]-Orderable unit MUST be coded according to the UN/ECE Recommendation 20</Message></Context><Context mark="fatal" values="CurrencyCodeList" address="@currencyID"><Message>[OP-T19-R011]-currencyID MUST be coded using ISO code list 4217</Message></Context><Context mark="fatal" values="Language" address="@languageID"><Message>[OP-T19-R012]-languageID MUST be coded using ISO code list 639-1</Message></Context><Context mark="fatal" values="PartyIdentifierScheme" address="cbc:CompanyID/@schemeID"><Message>[OP-T19-R013]-A Party Company Identifier Scheme MUST be from the list of PEPPOL Party Identifiers described in the &quot;PEPPOL Policy for using Identifiers&quot;.</Message></Context></Contexts></ns2:ContextValueAssociation>