<?xml version="1.0" encoding="UTF-8" standalone="yes"?><ns2:ContextValueAssociation xmlns:ns2="http://docs.oasis-open.org/codelist/ns/ContextValueAssociation/1.0/" name="OPENPEPPOL-T01"><ValueLists><ValueList uri="../gc/OPENPEPPOL-BinaryObjectMimeCode.gc" xml:id="BinaryObjectMimeCode"/><ValueList uri="../gc/OPENPEPPOL-CountryCodeList.gc" xml:id="CountryCodeList"/><ValueList uri="../gc/OPENPEPPOL-CurrencyCodeList.gc" xml:id="CurrencyCodeList"/><ValueList uri="../gc/OPENPEPPOL-DocumentTypeCode.gc" xml:id="DocumentTypeCode"/><ValueList uri="../gc/OPENPEPPOL-PartyIdentifierScheme.gc" xml:id="PartyIdentifierScheme"/><ValueList uri="../gc/OPENPEPPOL-TaxCategoryID.gc" xml:id="TaxCategoryID"/><ValueList uri="../gc/OPENPEPPOL-UnitCode.gc" xml:id="UnitCode"/></ValueLists><Contexts><Context mark="fatal" values="DocumentTypeCode" address="cbc:OrderTypeCode"><Message>[OP-T01-R001]-OrderTypeCode in an order MUST be from UN/ECE 1001 code list PEPPOL Subset</Message></Context><Context mark="fatal" values="CurrencyCodeList" address="cbc:DocumentCurrencyCode"><Message>[OP-T01-R002]-DocumentCurrencyCode MUST be coded using ISO code list 4217</Message></Context><Context mark="fatal" values="BinaryObjectMimeCode" address="@mimeCode"><Message>[OP-T01-R003]-A Binary Object MIME code attribute MUST be coded using MIME Code Type version 2008 code list</Message></Context><Context mark="fatal" values="PartyIdentifierScheme" address="cbc:EndpointID/@schemeID"><Message>[OP-T01-R004]-An Enpoint Identifier Scheme identifier MUST be from the list of PEPPOL Party Identifiers described in the &quot;PEPPOL Policy for using Identifiers&quot;.</Message></Context><Context mark="fatal" values="PartyIdentifierScheme" address="cac:PartyIdentification/cbc:ID/@schemeID"><Message>[OP-T01-R005]-An Party Identifier Scheme identifier MUST be from the list of PEPPOL Party Identifiers described in the &quot;PEPPOL Policy for using Identifiers&quot;.</Message></Context><Context mark="fatal" values="CountryCodeList" address="cac:Country/cbc:IdentificationCode"><Message>[OP-T01-R006]-Country codes in an order MUST be coded using ISO code list 3166-1</Message></Context><Context mark="fatal" values="CurrencyCodeList" address="@currencyID"><Message>[OP-T01-R007]-currencyID MUST be coded using ISO code list 4217</Message></Context><Context mark="fatal" values="TaxCategoryID" address="cac:TaxCategory/cbc:ID/"><Message>[OP-T01-R008]-A tax category identifier MUST be coded using UN/ECE 5305 BII2 Subset</Message></Context><Context mark="fatal" values="UnitCode" address="@unitCode"><Message>[OP-T01-R009]-Unit code MUST be coded according to the UN/ECE Recommendation 20 (2009)</Message></Context></Contexts></ns2:ContextValueAssociation>