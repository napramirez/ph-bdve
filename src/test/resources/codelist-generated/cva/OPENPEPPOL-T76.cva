<?xml version="1.0" encoding="UTF-8" standalone="yes"?><ns2:ContextValueAssociation xmlns:ns2="http://docs.oasis-open.org/codelist/ns/ContextValueAssociation/1.0/" name="OPENPEPPOL-T76"><ValueLists><ValueList uri="../gc/OPENPEPPOL-CurrencyCodeList.gc" xml:id="CurrencyCodeList"/><ValueList uri="../gc/OPENPEPPOL-OrderResponseCode.gc" xml:id="OrderResponseCode"/><ValueList uri="../gc/OPENPEPPOL-PartyIdentifierScheme.gc" xml:id="PartyIdentifierScheme"/><ValueList uri="../gc/OPENPEPPOL-TaxCategoryID.gc" xml:id="TaxCategoryID"/><ValueList uri="../gc/OPENPEPPOL-UnitCode.gc" xml:id="UnitCode"/></ValueLists><Contexts><Context mark="fatal" values="OrderResponseCode" address="cbc:OrderResponseCode"><Message>[OP-T76-R001]-An order response code MUST be coded according to the code list 1225.</Message></Context><Context mark="fatal" values="OrderResponseCode" address="cac:LineItem/cbc:LineStatusCode"><Message>[OP-T76-R002]-An order response line status code MUST be coded according to the code list 1225.</Message></Context><Context mark="fatal" values="CurrencyCodeList" address="cbc:DocumentCurrencyCode"><Message>[OP-T76-R003]-DocumentCurrencyCode MUST be coded using ISO code list 4217</Message></Context><Context mark="fatal" values="PartyIdentifierScheme" address="cbc:EndpointID/@schemeID"><Message>[OP-T76-R004]-An Enpoint Identifier Scheme identifier MUST be from the list of PEPPOL Party Identifiers described in the &quot;PEPPOL Policy for using Identifiers&quot;.</Message></Context><Context mark="fatal" values="PartyIdentifierScheme" address="cac:PartyIdentification/cbc:ID/@schemeID"><Message>[OP-T76-R005]-An Party Identifier Scheme identifier MUST be from the list of PEPPOL Party Identifiers described in the &quot;PEPPOL Policy for using Identifiers&quot;.</Message></Context><Context mark="fatal" values="UnitCode" address="@unitCode"><Message>[OP-T76-R006]-Unit code MUST be coded according to the UN/ECE Recommendation 20 (2009)</Message></Context><Context mark="fatal" values="CurrencyCodeList" address="@currencyID"><Message>[OP-T76-R007]-CurrencyID MUST be coded using ISO code list 4217</Message></Context><Context mark="fatal" values="TaxCategoryID" address="cac:TaxCategory/cbc:ID"><Message>[OP-T76-R008]-A tax category identifier MUST be coded using UN/ECE 5305 BII2 Subset</Message></Context></Contexts></ns2:ContextValueAssociation>