<?xml version="1.0" encoding="UTF-8" standalone="yes"?><ns2:ContextValueAssociation xmlns:ns2="http://docs.oasis-open.org/codelist/ns/ContextValueAssociation/1.0/" name="OPENPEPPOL-T01"><ValueLists><ValueList xml:id="BinaryObjectMimeCode" uri="../gc/OPENPEPPOL-BinaryObjectMimeCode.gc"/><ValueList xml:id="CountryCodeList" uri="../gc/OPENPEPPOL-CountryCodeList.gc"/><ValueList xml:id="CurrencyCodeList" uri="../gc/OPENPEPPOL-CurrencyCodeList.gc"/><ValueList xml:id="DocumentTypeCode" uri="../gc/OPENPEPPOL-DocumentTypeCode.gc"/><ValueList xml:id="PartyIdentifierScheme" uri="../gc/OPENPEPPOL-PartyIdentifierScheme.gc"/><ValueList xml:id="TaxCategoryID" uri="../gc/OPENPEPPOL-TaxCategoryID.gc"/><ValueList xml:id="UnitCode" uri="../gc/OPENPEPPOL-UnitCode.gc"/></ValueLists><Contexts><Context address="cbc:OrderTypeCode" values="DocumentTypeCode" mark="fatal"><Message>[OP-T01-R001]-OrderTypeCode in an order MUST be from UN/ECE 1001 code list PEPPOL Subset</Message></Context><Context address="cbc:DocumentCurrencyCode" values="CurrencyCodeList" mark="fatal"><Message>[OP-T01-R002]-DocumentCurrencyCode MUST be coded using ISO code list 4217</Message></Context><Context address="@mimeCode" values="BinaryObjectMimeCode" mark="fatal"><Message>[OP-T01-R003]-A Binary Object MIME code attribute MUST be coded using MIME Code Type version 2008 code list</Message></Context><Context address="cbc:EndpointID/@schemeID" values="PartyIdentifierScheme" mark="fatal"><Message>[OP-T01-R004]-An Enpoint Identifier Scheme identifier MUST be from the list of PEPPOL Party Identifiers described in the "PEPPOL Policy for using Identifiers".</Message></Context><Context address="cac:PartyIdentification/cbc:ID/@schemeID" values="PartyIdentifierScheme" mark="fatal"><Message>[OP-T01-R005]-An Party Identifier Scheme identifier MUST be from the list of PEPPOL Party Identifiers described in the "PEPPOL Policy for using Identifiers".</Message></Context><Context address="cac:Country/cbc:IdentificationCode" values="CountryCodeList" mark="fatal"><Message>[OP-T01-R006]-Country codes in an order MUST be coded using ISO code list 3166-1</Message></Context><Context address="@currencyID" values="CurrencyCodeList" mark="fatal"><Message>[OP-T01-R007]-currencyID MUST be coded using ISO code list 4217</Message></Context><Context address="cac:TaxCategory/cbc:ID" values="TaxCategoryID" mark="fatal"><Message>[OP-T01-R008]-A tax category identifier MUST be coded using UN/ECE 5305 BII2 Subset</Message></Context><Context address="@unitCode" values="UnitCode" mark="fatal"><Message>[OP-T01-R009]-Unit code MUST be coded according to the UN/ECE Recommendation 20 (2009)</Message></Context></Contexts></ns2:ContextValueAssociation>