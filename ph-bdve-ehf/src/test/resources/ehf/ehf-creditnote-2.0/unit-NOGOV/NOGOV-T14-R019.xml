			<cac:CreditNoteLine>
				<cac:Item>
					<cac:CommodityClassification>
						<!-- <cbc:ItemClassificationCode/> -->
					</cac:CommodityClassification>
				</cac:Item>
			</cac:CreditNoteLine>

		</CreditNote>
