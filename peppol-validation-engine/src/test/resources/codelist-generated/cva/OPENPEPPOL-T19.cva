<?xml version="1.0" encoding="UTF-8" standalone="yes"?><ns2:ContextValueAssociation xmlns:ns2="http://docs.oasis-open.org/codelist/ns/ContextValueAssociation/1.0/" name="OPENPEPPOL-T19"><ValueLists><ValueList xml:id="ActionCodePEPPOL" uri="../gc/OPENPEPPOL-ActionCodePEPPOL.gc"/><ValueList xml:id="AttributeCode" uri="../gc/OPENPEPPOL-AttributeCode.gc"/><ValueList xml:id="CountryCodeList" uri="../gc/OPENPEPPOL-CountryCodeList.gc"/><ValueList xml:id="CurrencyCodeList" uri="../gc/OPENPEPPOL-CurrencyCodeList.gc"/><ValueList xml:id="Language" uri="../gc/OPENPEPPOL-Language.gc"/><ValueList xml:id="PackageLevelCode" uri="../gc/OPENPEPPOL-PackageLevelCode.gc"/><ValueList xml:id="PartyIdentifierScheme" uri="../gc/OPENPEPPOL-PartyIdentifierScheme.gc"/><ValueList xml:id="UNDG" uri="../gc/OPENPEPPOL-UNDG.gc"/><ValueList xml:id="UnitCode" uri="../gc/OPENPEPPOL-UnitCode.gc"/></ValueLists><Contexts><Context address="cac:Country/cbc:IdentificationCode" values="CountryCodeList" mark="fatal"><Message>[OP-T19-R001]-Country codes in an invoice MUST be coded using ISO code list 3166-1</Message></Context><Context address="@unitCode" values="UnitCode" mark="fatal"><Message>[OP-T19-R002]-Unit code MUST be coded according to the UN/ECE Recommendation 20</Message></Context><Context address="cbc:UNDGCode" values="UNDG" mark="fatal"><Message>[OP-T19-R003]-UNDG MUST be coded according to the UNCL 8273 code list</Message></Context><Context address="cbc:EndpointID/@schemeID" values="PartyIdentifierScheme" mark="fatal"><Message>[OP-T19-R005]-An Endpoint Identifier Scheme MUST be from the list of PEPPOL Party Identifiers described in the "PEPPOL Policy for using Identifiers".</Message></Context><Context address="cac:PartyIdentification/cbc:ID/@schemeID" values="PartyIdentifierScheme" mark="fatal"><Message>[OP-T19-R006]-A Party Identifier Scheme MUST be from the list of PEPPOL Party Identifiers described in the "PEPPOL Policy for using Identifiers".</Message></Context><Context address="/ubl:Catalogue/cbc:ActionCode" values="ActionCodePEPPOL" mark="fatal"><Message>[OP-T19-R007]-An action code at header level MUST be from the PEPPOL action code list.</Message></Context><Context address="cbc:PackageLevelCode" values="PackageLevelCode" mark="fatal"><Message>[OP-T19-R008]-A package level code MUST be from the PEPPOL subset of GS1 7009 code list.</Message></Context><Context address="cbc:AttributeID" values="AttributeCode" mark="fatal"><Message>[OP-T19-R009]-An attribute identifier MUST be from the UN EDIFACT 6313 code list</Message></Context><Context address="cbc:OrderableUnit" values="UnitCode" mark="fatal"><Message>[OP-T19-R010]-Orderable unit MUST be coded according to the UN/ECE Recommendation 20</Message></Context><Context address="@currencyID" values="CurrencyCodeList" mark="fatal"><Message>[OP-T19-R011]-currencyID MUST be coded using ISO code list 4217</Message></Context><Context address="@languageID" values="Language" mark="fatal"><Message>[OP-T19-R012]-languageID MUST be coded using ISO code list 639-1</Message></Context><Context address="cbc:CompanyID/@schemeID" values="PartyIdentifierScheme" mark="fatal"><Message>[OP-T19-R013]-A Party Company Identifier Scheme MUST be from the list of PEPPOL Party Identifiers described in the "PEPPOL Policy for using Identifiers".</Message></Context></Contexts></ns2:ContextValueAssociation>