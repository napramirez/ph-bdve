<?xml version="1.0" encoding="UTF-8" standalone="yes"?><ns2:ContextValueAssociation xmlns:ns2="http://docs.oasis-open.org/codelist/ns/ContextValueAssociation/1.0/" name="OPENPEPPOL-T76"><ValueLists><ValueList xml:id="CurrencyCodeList" uri="../gc/OPENPEPPOL-CurrencyCodeList.gc"/><ValueList xml:id="OrderResponseCode" uri="../gc/OPENPEPPOL-OrderResponseCode.gc"/><ValueList xml:id="PartyIdentifierScheme" uri="../gc/OPENPEPPOL-PartyIdentifierScheme.gc"/><ValueList xml:id="TaxCategoryID" uri="../gc/OPENPEPPOL-TaxCategoryID.gc"/><ValueList xml:id="UnitCode" uri="../gc/OPENPEPPOL-UnitCode.gc"/></ValueLists><Contexts><Context address="cbc:OrderResponseCode" values="OrderResponseCode" mark="fatal"><Message>[OP-T76-R001]-An order response code MUST be coded according to the code list 1225.</Message></Context><Context address="cac:LineItem/cbc:LineStatusCode" values="OrderResponseCode" mark="fatal"><Message>[OP-T76-R002]-An order response line status code MUST be coded according to the code list 1225.</Message></Context><Context address="cbc:DocumentCurrencyCode" values="CurrencyCodeList" mark="fatal"><Message>[OP-T76-R003]-DocumentCurrencyCode MUST be coded using ISO code list 4217</Message></Context><Context address="cbc:EndpointID/@schemeID" values="PartyIdentifierScheme" mark="fatal"><Message>[OP-T76-R004]-An Enpoint Identifier Scheme identifier MUST be from the list of PEPPOL Party Identifiers described in the "PEPPOL Policy for using Identifiers".</Message></Context><Context address="cac:PartyIdentification/cbc:ID/@schemeID" values="PartyIdentifierScheme" mark="fatal"><Message>[OP-T76-R005]-An Party Identifier Scheme identifier MUST be from the list of PEPPOL Party Identifiers described in the "PEPPOL Policy for using Identifiers".</Message></Context><Context address="@unitCode" values="UnitCode" mark="fatal"><Message>[OP-T76-R006]-Unit code MUST be coded according to the UN/ECE Recommendation 20 (2009)</Message></Context><Context address="@currencyID" values="CurrencyCodeList" mark="fatal"><Message>[OP-T76-R007]-CurrencyID MUST be coded using ISO code list 4217</Message></Context><Context address="cac:TaxCategory/cbc:ID" values="TaxCategoryID" mark="fatal"><Message>[OP-T76-R008]-A tax category identifier MUST be coded using UN/ECE 5305 BII2 Subset</Message></Context></Contexts></ns2:ContextValueAssociation>